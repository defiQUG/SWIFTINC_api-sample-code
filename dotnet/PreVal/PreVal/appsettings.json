{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient.PreValApiClientExt.ClientHandler": "Trace",
      "System.Net.Http.HttpClient.OAuthTokenService.ClientHandler": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "BaseAddress": "https://sandbox.swift.com",
    "Endpoint": "/swift-preval/v2/",
    "TokenUri": "/oauth2/v1/token",
    "TokenTimeSkewInSeconds": "5",
    "GrantType": "urn:ietf:params:oauth:grant-type:jwt-bearer",
    "Scope": "swift.preval!p",
    "KeyStore": "sandbox_keystore.pfx",
    "KeyStorePassword": "Abcd1234",
    "ProxyHost": "",
    "ProxyPort": "8080"
  }
}
